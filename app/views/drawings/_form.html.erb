<%= form_for @drawing do |f| %>
    <%= render 'shared/error_messages', object: @drawing %>
    
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    
    <%= f.label :program %>
    <%= f.text_field :program %>
    <br>
    
    <%= f.label :attachment %>
    <%= f.file_field :attachment %>
    
    <%= f.submit "Save", class: "btn btn-primary" %>

<% end %>